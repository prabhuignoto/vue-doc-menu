{
  "name": "vue-dock-menu",
  "version": "1.1.0",
  "license": "MIT",
  "author": "Prabhu Murthy",
  "description": "Dockable menu bar for Vue 3",
  "keywords": [
    "menu",
    "dock-menu",
    "vue-menu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/prabhuignoto/vue-dock-menu"
  },
  "scripts": {
    "dev": "vite",
    "rollup": "rimraf ./dist && rollup -c",
    "eslint": "eslint src/**/*.vue",
    "lint:css": "stylelint src/**/*.vue --custom-syntax postcss-html",
    "lint:all": "yarn eslint && yarn lint:css",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "stylelint src/**/*.scss --fix",
      "git add"
    ],
    "src/**/*.vue": [
      "eslint src/**/*.vue --fix",
      "git add"
    ]
  },
  "dependencies": {
    "focus-visible": "^5.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-sucrase": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/compiler-sfc": "^3.2.33",
    "eslint": "^8.15.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "postcss-html": "^1.4.1",
    "rollup": "^2.72.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.51.0",
    "stylelint": "^14.8.2",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vue": "^3.2.33"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.0.5",
    "vue": "^3.0.5"
  },
  "main": "dist/vue-dock-menu.js",
  "module": "dist/vue-dock-menu.es.js",
  "umd": "dist/vue-dock-menu.umd.js",
  "files": [
    "dist"
  ]
}
