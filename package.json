{
  "name": "vue-dock-menu",
  "version": "0.2.1",
  "license": "MIT",
  "author": "Prabhu Murthy",
  "description": "Dockable menu bar for Vue 3",
  "keywords": [
    "menu",
    "dock-menu",
    "vue-menu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/prabhuignoto/vue-dock-menu"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "rollup": "rimraf ./dist && rollup -c",
    "eslint": "eslint src/**/*.vue",
    "lint:css": "stylelint src/**/*.vue",
    "lint:all": "yarn eslint && yarn lint:css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "stylelint src/**/*.scss --fix",
      "git add"
    ],
    "src/**/*.vue": [
      "eslint src/**/*.vue --fix",
      "git add"
    ]
  },
  "dependencies": {
    "focus-visible": "^5.2.0"
  },
  "devDependencies": {
    "@interactjs/types": "^1.10.0",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "@vue/compiler-sfc": "^3.0.3",
    "eslint": "^7.14.0",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "rollup": "^2.33.3",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0-beta.11",
    "sass": "^1.29.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.1.2",
    "vite": "^1.0.0-rc.9",
    "vue": "^3.0.3"
  },
  "peerDependencies": {},
  "main": "dist/vue-dock-menu.js",
  "module": "dist/vue-dock-menu.es.js",
  "umd": "dist/vue-dock-menu.umd.js",
  "files": [
    "dist"
  ]
}
