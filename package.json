{
  "name": "vue-dock-menu",
  "version": "1.0.3",
  "license": "MIT",
  "author": "Prabhu Murthy",
  "description": "Dockable menu bar for Vue 3",
  "keywords": [
    "menu",
    "dock-menu",
    "vue-menu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/prabhuignoto/vue-dock-menu"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "rollup": "rimraf ./dist && rollup -c",
    "eslint": "eslint src/**/*.vue",
    "lint:css": "stylelint src/**/*.vue",
    "lint:all": "yarn eslint && yarn lint:css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "stylelint src/**/*.scss --fix",
      "git add"
    ],
    "src/**/*.vue": [
      "eslint src/**/*.vue --fix",
      "git add"
    ]
  },
  "dependencies": {
    "focus-visible": "^5.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vue/compiler-sfc": "^3.1.5",
    "eslint": "^7.30.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.1",
    "rollup": "^2.53.2",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.35.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.2",
    "vue": "^3.1.5"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.0.5",
    "vue": "^3.0.5"
  },
  "main": "dist/vue-dock-menu.js",
  "module": "dist/vue-dock-menu.es.js",
  "umd": "dist/vue-dock-menu.umd.js",
  "files": [
    "dist"
  ]
}
